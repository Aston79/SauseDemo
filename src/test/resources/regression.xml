<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="4" name="Test suite" parallel="tests">
    <listeners>
        <listener class-name="tests.base.AnnotationTransformer"/>
    </listeners>
    <test name="test 1">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.FramesTest"/>
            <class name="tests.ContextMenuTest"/>
            <class name="tests.DynamicControlTest"/>
        </classes>
    </test> <!-- Test -->

    <test name="test 2">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="tests.CartPageTest"/>
            <class name="tests.ContextMenuTest"/>
            <class name="tests.ProductCriticalPathTest"/>
        </classes>
    </test> <!-- Test -->

</suite>